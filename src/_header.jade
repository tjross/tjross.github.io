.header.navbar.navbar-default.navbar-fixed-top(role='navigation')
  .container-fluid

    // Site logo
    .navbar-header.pull-left
      .nav.navbar-left.site-logo
        a(href='index.html')
          img(alt='Logo', src='icons/Main_nav_logo_blk_180px.png')

    // Log-in
    .navbar-header.pull-right
      ul.nav.navbar-nav.pull-left
        li
          button.btn.btn-default.navbar-btn(href='#') Log In

      // Collapsed links dropdown
      button.navbar-toggle.collapsed(type="button", data-toggle="collapse", data-target="#collapsed-nav")
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar

    // Links
    .collapse.navbar-collapse#collapsed-nav
      ul.nav.navbar-nav.navbar-right
        li
          a(href='design.html') Design
        li
          a(href='about.html') About
        li
          a(href='faq.html') FAQ
        li
          a(href='blog.html') Blog
